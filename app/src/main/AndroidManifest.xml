<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gab.mobility_final">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/web_hi_res_512_orange"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".IPActivity" android:label="Mobility">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:label="Trackpad">

        </activity>
        <activity android:name=".SelectActivity" android:label="Getting Started"></activity>
        <activity android:name=".Presenter" android:label="Presenter"></activity>
        <activity android:name=".Splash"></activity>
        <activity android:name=".Keyboard" android:label="Keyboard"></activity>
    </application>

</manifest>